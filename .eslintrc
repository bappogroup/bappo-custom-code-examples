{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "arrow-parens": ["off"],
    "camelcase": "off",
    "max-len": "off",
    "react/jsx-filename-extension": ["off"],
    "react/prop-types": ["off"],
    "import/no-unresolved": ["off"],
    "import/extensions": ["off"],
    "no-plusplus": "off",
    "no-restricted-syntax": "off"
  }
}